{
  "name": "status-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "keywords": [
    "react",
    "expo",
    "template",
    "typescript",
    "nativebase"
  ],
  "license": "MIT",
  "scripts": {
    "android": "expo start --android",
    "depcheck": "depcheck",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "fix": "yarn eslint --fix",
    "lint": "yarn eslint",
    "eslint": "eslint --max-warnings=0 --ext .js,.ts,.jsx,.tsx .",
    "shake": "./node_modules/.bin/ts-unused-exports ./tsconfig.json --excludePathsFromReport=\"generated;App.tsx;codegen.ts\"",
    "start": "expo start",
    "web": "expo start --web",
    "prepare": "husky install",
    "schema:generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@supabase/supabase-js": "^2.2.2",
    "expo": "^44.0.0",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "native-base": "3.4.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-elements": "^3.4.2",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.2",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native-a11y": "^3.2.1",
    "expo-cli": "^6.0.5",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-unused-exports": "^8.0.0",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "@types/react": "^17.0.47"
  },
  "bugs": {
    "url": "https://github.com/GeekyAnts/nativebase-templates/issues"
